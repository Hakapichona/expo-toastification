{
  "compilerOptions": {
    "target": "ES2019",
    "module": "NodeNext",
    "lib": ["ES2019"],
    "jsx": "react-jsx",
    "declaration": true,
    "strict": true,
    "moduleResolution": "NodeNext",
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "noImplicitAny": true,
  },
  "include": ["src"]
}
